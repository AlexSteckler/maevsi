{
  "extends": [
    "github>dargmuesli/renovate-config"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["chalk"],
      "allowedVersions": "<4"
    },
    {
      "matchPackageNames": ["css-loader"],
      "allowedVersions": "<6"
    },
    {
      "matchPackageNames": ["mustache"],
      "allowedVersions": "<4"
    },
    {
      "matchPackageNames": ["qrcode.vue"],
      "allowedVersions": "<2"
    },
    {
      "matchPackageNames": ["react", "react-dom"],
      "allowedVersions": "<17"
    },
    {
      "matchPackageNames": ["vue"],
      "allowedVersions": "<3"
    },
    {
      "matchPackageNames": ["vuex"],
      "allowedVersions": "<4"
    },
    {
      "matchPackageNames": ["@types/webpack", "webpack"],
      "allowedVersions": "<5"
    }
  ]
}
